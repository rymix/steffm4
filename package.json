    "axios": "^1.1.3",
    "date-fns": "^3.3.1",
    "framer-motion": "^10.16.16",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "moment": "^2.29.4",
    "next": "^14.0.4",
    "polished": "^4.2.2",
