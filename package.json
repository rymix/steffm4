{
  "name": "steffm4",
  "version": "1.0.0",
  "description": "Stef.FM - Funky House and Soul Music - Adventures in Decent Music",
  "author": "Steve Arnott",
  "license": "MIT",
  "browserslist": [
    "defaults and supports es6 and not ie 11"
  ],
  "scripts": {
    "build": "next build && node post-build.js",
    "dev": "set PORT=3001&&next dev",
    "devmac": "PORT=3001 next dev",
    "eslint": "eslint .",
    "export": "next export -o _static",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "start": "PORT=8080 node .next/standalone/node/steffm4/server.js",
    "startwin": "set PORT=8080&&node .next/standalone/server.js",
    "startnext": "node_modules/next/dist/bin/next start",
    "stylelint": "stylelint --formatter=verbose --fix '**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "resolutions": {
    "styled-components": "^6.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rymix/steffm4.git"
  },
